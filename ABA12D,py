arr=[2,4,9,16,25,64,289,729,1681,2401,3481,4096,5041,7921,10201,15625, 17161, 27889, 28561, 29929, 65536, 83521, 85849, 146689, 262144, 279841, 458329, 491401, 531441, 552049, 579121, 597529, 683929, 703921, 707281, 734449, 829921, 1190281]
for _ in input()*[0]:
    a=list(int(i) for i in raw_input().split())
    cnt=0
    for i in arr:
        if i>=a[0] and i<=a[i]:cnt+=1
    print cnt
